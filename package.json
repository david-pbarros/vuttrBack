        "express-load": "^1.1.16",
        "helmet": "^3.15.1",
        "jwt-simple": "^0.5.5",
        "mysql2": "^1.6.5",
        "passport": "^0.4.0",
        "passport-google-oauth": "^1.0.0",
        "passport-jwt": "^4.0.0",
        "sequelize": "^4.42.0",
        "sequelize-cli": "^5.4.0"
    },
    "devDependencies": {
        "nodemon": "^1.18.10"
    }
